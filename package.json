{
  "name": "repo-analyzer",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-native": "electron-build-env neon build --release",
    "build-main": "cross-env NODE_ENV=development webpack --config ./configs/webpack.config.main.babel.js --colors",
    "build-renderer": "cross-env NODE_ENV=development webpack --config ./configs/webpack.config.renderer.babel.js --colors",
    "build": "concurrently 'npm run build-main' 'npm run build-renderer'",
    "clean-native-build": "neon clean",
    "start-main-dev": "cross-env NODE_ENV=development electron -r @babel/register ./app/main.js",
    "start-renderer-dev": "cross-env NODE_ENV=development webpack-dev-server --config configs/webpack.config.renderer.babel.js",
    "start": "electron app/main.build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/str1x/repo-analyzer.git"
  },
  "author": "Max Green <strixdevelop@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/str1x/repo-analyzer/issues"
  },
  "homepage": "https://github.com/str1x/repo-analyzer#readme",
  "dependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "electron": "^9.0.2",
    "electron-log": "^4.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/register": "^7.10.1",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "electron-build-env": "^0.2.0",
    "electron-devtools-installer": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "neon-cli": "^0.4.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "terser-webpack-plugin": "^3.0.3",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
